{
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
},
"hidefiles.configurationType": "local",
"hidefiles.globalConfig": {
    "profiles": [
        {
            "name": "Default",
            "detail": "",
            "description": "Test",
            "hidden": [
                ".vscode/"
            ]
        },
        {
            "name": "Strict",
            "detail": "Hide everything not immediately required",
            "hidden": [
                "$Default",
                "build/",
                "dist/",
                "out/",
                "node_modules/",
                ".editorconfig",
                ".eslintignore",
                ".eslintrc.cjs",
                ".gitignore",
                ".prettierrc.yaml",
                ".prettierignore",
                "LICENSE",
                "electron-builder.yml",
                "package-lock.json",
                "src/main/tsconfig.json",
                "src/preload/tsconfig.json",
                "src/tsconfig.json",
                "src/renderer/tsconfig.json"
            ]
        }
    ]
},
"files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    ".vscode/": true,
    "build/": true,
    "dist/": true,
    "out/": true,
    "node_modules/": true,
    ".editorconfig": true,
    ".eslintignore": true,
    ".eslintrc.cjs": true,
    ".gitignore": true,
    ".prettierrc.yaml": true,
    ".prettierignore": true,
    "LICENSE": true,
    "electron-builder.yml": true,
    "package-lock.json": true,
    "src/main/tsconfig.json": true,
    "src/preload/tsconfig.json": true,
    "src/tsconfig.json": true,
    "src/renderer/tsconfig.json": true
},
"hidefiles.selectedProfile": "Strict"
}
